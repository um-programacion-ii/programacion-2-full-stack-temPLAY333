# ===================================================================
# EJEMPLO DE VARIABLES DE ENTORNO
# Copiar este archivo como .env y completar con valores reales
# ===================================================================

# JWT Token para autenticación con servicios externos
JWT_TOKEN=your_jwt_token_here

# Servidor externo (Cátedra)
SERVER_URL=http://your-server-url:8080
API_BASE=/api

# Credenciales de usuario para servicios externos
USERNAME=your_username
PASSWORD=your_password

# ===================================================================
# BASE DE DATOS - DESARROLLO
# ===================================================================
DB_DEV_HOST=localhost
DB_DEV_PORT=3306
DB_DEV_NAME=MicroservicesFinal
DB_DEV_USERNAME=your_username
DB_DEV_PASSWORD=your_password

# ===================================================================
# BASE DE DATOS - PRODUCCIÓN
# ===================================================================
DB_PROD_HOST=localhost
DB_PROD_PORT=3307
DB_PROD_NAME=MicroservicesFinal_prod
DB_PROD_USERNAME=your_username
DB_PROD_PASSWORD=your_password

# ===================================================================
# REDIS
# ===================================================================
REDIS_HOST=localhost
REDIS_PORT=6379

